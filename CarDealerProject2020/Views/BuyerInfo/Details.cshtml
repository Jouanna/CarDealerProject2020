@model BuyerInfoEditViewModel
@{
    ViewData["Title"] = "Детайли";
}

<h1>@ViewData["Title"]</h1>

<head>
    <style>
        body {
            background-color: MintCream;
        }
    </style>
</head>

<p>Номер - @Model.Id</p>
<p>Име - @Model.FirstName</p>
<p>Презиме - @Model.MiddleName</p>
<p>Фамилия - @Model.LastName</p>
<p>Населено място - @Model.City</p>
<p>Адрес - @Model.Address</p>
<p>Телефонен номер - @Model.Phone</p>
<p>Номер на шофьорска книжка - @Model.DriverLicenseNumber</p>

<a asp-action="Index" class="btn btn-success">Назад</a>



